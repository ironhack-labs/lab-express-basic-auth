{
  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "nodemon",
      "runtimeExecutable": "nodemon",
      "program": "${workspaceFolder}/bin/www",
      "restart": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    

    {
      "type": "node",
      "request": "attach",
      "name": "DEBUG Irongenerate",
      "port": 9229,
      "protocol": "inspector",
      "restart": "true"
    }
  ]
}
