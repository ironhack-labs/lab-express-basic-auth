<h1> SIGN UP MATE </h1>
<form action='/signup' method='POST'>

  <label for='username'>Username</label>
  <input type='text' name='username'>

  <label for='password'>Password</label>
  <input type='password' name='password'>

  <button type='submit'>Submit</button>
</form>

{{#if errorMessage}}
<p>{{errorMessage}}</p>
{{/if}}

