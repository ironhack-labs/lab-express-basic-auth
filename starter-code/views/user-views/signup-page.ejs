<h2>Sign Up</h2>

<% if (typeof(errorMessage) != "undefined") { %>
  <div class="error-message"><%= errorMessage %></div>
<% } %>


<form id= "form" class="" action="process-signup" method="post">

  <label for="username-input">UserName</label>
  <input id="username-input" type="text" name="signupUserName" value="">



  <label for="password-input">Password</label>
  <input id="password-input" type="password" name="signupPassword" value="">


  <button>Sign Up</button>
</form>
