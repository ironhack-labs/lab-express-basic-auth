<h1>Sign up here!</h1>

<form action="/signup" method="post">
    <label for="">Username</label>
    <input type="text" name="username">

    <label for="">Password</label>
    <input type="password" name="password">
    <button>Sign up!</button>
</form>

{{#if errorMessage}}
<p>{{errorMessage}}</p>
{{/if}}