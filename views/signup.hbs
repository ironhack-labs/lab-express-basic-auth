<div id="form">
  <h2>Sign Up</h2>
  <form action="/signup" method="POST">
    <label>
      Username
      <input type="text" name="username" />
    </label>
    <label>
      Password
      <input type="password" name="password" />
    </label>
    <button type="submit">Create your Account</button>
  </form>
</div>